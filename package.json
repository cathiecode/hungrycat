    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@fastify/request-context": "^4.0.0",
    "date-fns": "^2.29.1",
    "fastify": "^4.3.0",
    "node-fetch": "^3.2.9"
